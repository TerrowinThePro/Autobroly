Whitelist = {
  293921182,
  506683401
} 
PrivateWhitelist = {
  293921182,
  506683401
}
local UserId = game.Players.LocalPlayer.UserId
local Whitelisted = false
local PrivateWhitelisted = false

	if table.find(Whitelist, UserId) then
		Whitelisted = true
		print("Whitelisted")
	end

  if table.find(PrivateWhitelist, UserId) then
    PrivateWhitelisted = true
    print("Private Whitelisted")
  end

wait()
if Whitelisted == false then
	game.Players.LocalPlayer:Kick("You're not whitelisted")
end
